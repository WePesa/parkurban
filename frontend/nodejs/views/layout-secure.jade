doctype 5
html
  head
    title= "ParkUrban "
    meta(name='viewport', content='width=device-width, initial-scale=1')
    //link(rel='stylesheet', href='/libs/bootstrap/css/bootstrap.min.css')
    link(rel='stylesheet', type='text/css', href='/libs/jqmobile/jquery.mobile-1.2.0.min.css')
    link(rel='stylesheet', type='text/css', href='/stylesheets/style.css')
    block styles
    
    script(type='text/javascript',src='/libs/jqmobile/jquery-1.8.2.min.js')
    script(type='text/javascript',src='/libs/jqmobile/jquery.mobile-1.2.0.min.js')
    script(type='text/javascript',src='/javascripts/secure.js')
    block scripts
    script(type='text/javascript')
      var activeTab = '#{activeTab}'
  body
    div(data-role='page')
      div(data-role='header')
        a(href='/home', data-role='button', data-icon='home') Home
        a(href='/search', data-role='button', data-icon='search', data-iconpos='right', data-ajax='false') Search
        h1=pageLabel
        div(data-role='navbar',data-iconpos='top')
          ul
            li: a#nav-latest(href='/home') Latest
            li: a#nav-locations(href='/locations') Locations
            li: a#nav-revenue(href='/revenue') Revenue
            li: a#nav-settings(href='/settings') Settings
        block header
      div(data-role='content')
        block content
      div(data-role='footer')
        p.centered ParkUrban (alpha)
        block footer

